<script type="text/javascript">
/* <![CDATA[ */
if (require) {require(['ta/rollupAmdShim'], function(rollupAmdShim) { rollupAmdShim.install([], ["ta"]); });
}
else {if (window.ta&&ta.rollupAmdShim) {ta.rollupAmdShim.install([],["ta"]);}
