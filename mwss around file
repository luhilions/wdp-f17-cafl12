require(['ta/p13n/placements'], function(placements) {
var define = placements.define.bind(placements,'tier_one_messaging','handlers',['handlers']);
/*
* Private js for the tier_one_messaging placement
*/
define(["placement", "lib/jquery-amd", 'ta/Core/TA.LocalStorage'],
function(placement, $, localStorage) {
var $placementEl = $('#' + placement.id);
function _dismissMessage(pageProperty, pageAction, productAttribute) {
_hidePlacement();
// Record the fact that this placement has been hidden before, so it wouldn't be displayed in the future.
_disableMessage();
// TA.Record.js is large - use trjs to defer loading it so as not to increase page load size (which would trigger alerts on pages like MW /Hotels Lite - see CRM-6405) - i.e. don't add it to the define list at the top.
require(["trjs!ta/Core/TA.Record"], function(taRecord) {
taRecord.trackEventOnPage(pageProperty, pageAction, productAttribute);
});
}
function _placementClicked(event){
// Track the click event properties
var targetLinkEl = $(event.currentTarget);
var property1 = targetLinkEl.data("clickProperty1");
var property2 = targetLinkEl.data("clickProperty2");
var servletName = targetLinkEl.data("servletName");
require(["trjs!ta/Core/TA.Record"], function(taRecord) {
// module, action, context, value, noninteraction
// pcb_campaign/pcb_dummy, shelf_click, servlet name, LW_Tier_one_click
taRecord.trackEventOnPage(property1, 'shelf_click', servletName + '|' +  property2);
});
// Do not show up this message again, the user has already clicked it.
_disableMessage();
}
// This function is for view tracking, which is not a preferred way though. But Keep it here in case.
function _trackPageProperties() {
// get the tracking page properties from the DOM
var property1 = $(".tier_one_messaging_container", $placementEl).data("pageProperty1");
var property2 = $(".tier_one_messaging_container", $placementEl).data("pageProperty2");
var servletName = $(".tier_one_messaging_container .targetLink", $placementEl).data("servletName");
require(["trjs!ta/Core/TA.Record"], function(taRecord) {
taRecord.trackEventOnPage(property1, 'view', servletName + '|' + property2);
});
}
/***********************************************
* To be executed upon loading the placement
***********************************************/
// Is local storage available?
if (localStorage.enabled) {
// Has this placement with the same message been dismissed or clicked before? If so, don't show it again
var thumbPrint =_getThumbPrint();
var storedThumbPrint;
if (thumbPrint) {
storedThumbPrint = localStorage.get(thumbPrint);
}
// it is possible that the local storage has undefined entry with a value true.
// so we must ALSO verify thumbPrint is valid, not undefined.
if (storedThumbPrint) {
// If we set up thumbPrint in the backend AND this placement has been either clicked or dismissed before. Do not display it again
_hidePlacement();
}
else {
// this placement has not been clicked nor dismissed before. Display and track page properties
_displayPlacement();
}
}
// get the thumbprint of the placement-message combination
function _getThumbPrint() {
return $(".thumbprint", $placementEl).val();
}
function _displayPlacement(){
$(".tier_one_messaging_container", $placementEl)
.removeClass("hidden");
}
// Hide the container div of the placement
function _hidePlacement() {
$(".tier_one_messaging_container", $placementEl)
.addClass("hidden");
}
function _disableMessage() {
var thumbPrint =_getThumbPrint();
// if thumbPrint is undefined, local storage should not be set.
if (localStorage.enabled && thumbPrint) {
localStorage.set(thumbPrint, "true");
}
}
/*
If the placement is intended to be displayed at a fixed location at the bottom of the page, make it so.
This couldn't be done in css, since we need to move the containing div, not that associated with the class.
*/
$(".show_at_the_bottom", $placementEl)
.css("position", "fixed")
.css("bottom", "0")
.css("left", "0")
.css("z-index", "99")
.css("width", "100%");
$('.tier_one_messaging_container a.targetLink', $placementEl).on("click", _placementClicked);
/***********************************************
* PUBLIC FUNCTIONS
***********************************************/
return {
dismiss: _dismissMessage,
}
});
});require(['ta/p13n/placements','$prp/tier_one_messaging/handlers'], function(placements, impl) {
window.ta.plc_tier_one_messaging_0_handlers = placements.load('tier_one_messaging','handlers.js', { 'name': 'tier_one_messaging', 'occurrence': 0, 'id': 'taplc_tier_one_messaging_0', 'location_id': 147395, 'servletClass': 'com.TripResearch.servlet.airfare.AllAirfares', 'servletName': 'Flights', 'modules': ["handlers"], 'params': {}, 'data': {}});});
