define('flights/flight_search_results/FlightSearchForm', ['ta', 'lib/jquery-amd'], function (ta, $) {
"use strict";
var FORM_CONTROL_IDS = {
FLIGHTS_ADULTS_NUMBERS: 'FLIGHTS_ADULTS_NUMBERS',
FLIGHTS_SENIORS_NUMBERS: 'FLIGHTS_SENIORS_NUMBERS',
FLIGHTS_CHILDREN_NUMBERS: 'FLIGHTS_CHILDREN_NUMBERS',
FLIGHTS_ADULTS_NUMBERS_LABEL: 'FLIGHTS_ADULTS_NUMBERS_LABEL',
FLIGHTS_SENIORS_NUMBERS_LABEL: 'FLIGHTS_SENIORS_NUMBERS_LABEL',
FLIGHTS_CHILDREN_NUMBERS_LABEL: 'FLIGHTS_CHILDREN_NUMBERS_LABEL',
FLIGHTS_TOTAL_TRAVELERS: 'FLIGHTS_TOTAL_TRAVELERS'
};
var MAX_TRAVELER_NUMBERS = {
FLIGHTS_ADULTS_NUMBERS: 6,
FLIGHTS_SENIORS_NUMBERS: 6,
FLIGHTS_CHILDREN_NUMBERS: 5
};
var TRIP_TYPES = {
ROUND_TRIP: 'roundTrip',
ONE_WAY: 'oneWay',
MULTI_CITY: 'multiCity'
};
var FlightSearchFormController = function () {
this.peopleStrings = [];
this.messages;
this.metaLang;
this.errors = {};
this.travelerPopup = false;
this.hideErrors = false;
this.lastDate = null;
var that = this;
var _canUseLocalStore = function () {
return ta.util.localstorage && ta.util.localstorage.canUseLocalStore();
};
var _findSelector = function (selector) {
return that.widget.element.querySelector(selector);
};
var _hideCalendar = function () {
if (window.calendar && window.calendar.flyout) {
window.calendar.flyout.hide(false);
}
};
var _typeahead = function (event, elem, typeAheadSelect, tripType) {
var autocompleters = [];
if (elem.hasClass('ghostText')) {
elem.setProperty('value', '');
elem.removeClass('ghostText');
}
_hideCalendar();
if (tripType === TRIP_TYPES.MULTI_CITY) {
var num = parseInt((elem.id).split('_')[1]);
if ((_findSelector('#mc_' + num + '_airportFromCode').value === '') && (_findSelector('#mc_' + (num - 1) + '_airportToCode').value !== '')) {
_findSelector('#mc_' + num + '_airportFrom').value = _findSelector('#mc_' + (num - 1) + '_airportTo').value;
_findSelector('#mc_' + num + '_airportFromCode').value = _findSelector('#mc_' + (num - 1) + '_airportToCode').value;
}
}
var _hideAc = function (e, c) {
autocompleters.forEach(function (ac) {
if (ac.choices === c) {
ac.fx.start(0);
}
});
};
var options = {
onShow: function () {
},
onHide: _hideAc
};
ta.call('ta.widgets.typeahead.factory.ne', event, elem, undefined, typeAheadSelect, options, autocompleters);
};
var _verifyChildrenAges = function () {
var foundError = false;
var ageSelectors = that.widgetElement.getElements('.singleChildAge');
Array.each(ageSelectors, function (selector) {
selector.removeClass('ageError');
});
_findSelector('#FLIGHTS_CHILDREN_AGE_DISCLAIMER').removeClass('error');
var childrenNumber = parseInt(_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_CHILDREN_NUMBERS_LABEL).innerHTML, 10);
for (var i = 1; i <= childrenNumber; i++) {
var age = _findSelector('#FLIGHTS_CHILD_AGE_' + i).innerHTML;
if (age === '&lt; 1') {
age = 0;
}
age = parseInt(age, 10);
if (isNaN(age)) {
_findSelector('#FLIGHTS_AGE_SELECTOR_' + i).addClass('ageError');
foundError = true;
}
}
if (foundError) {
_findSelector('#FLIGHTS_CHILDREN_AGE_DISCLAIMER').addClass('error');
}
return !foundError;
};
var _handleSelectChange = function (elem) {
if (elem.getChildren('select').length === 0 ||
elem.getChildren('.selectValue').length === 0 ||
elem.getChildren('.selectValueHidden').length === 0) {
return;
}
var select = elem.getChildren('select')[0];
var value = elem.getChildren('.selectValue')[0];
var hiddenValue = elem.getChildren('.selectValueHidden')[0];
value.innerHTML = select.options[select.selectedIndex].text;
if (hiddenValue) {
hiddenValue.value = select.options[select.selectedIndex].text;
_verifyChildrenAges();
}
};
var _enableSearchButton = function () {
_findSelector('#CHECK_FARES_BUTTON').addEvent('click', that.searchCallback);
};
var _disableSearchButton = function () {
_findSelector('#CHECK_FARES_BUTTON').removeEvent('click', that.searchCallback);
};
var _hideTravelersMessage = function () {
var elem = _findSelector('#FLIGHTS_MESSAGE_BOX');
if (!elem) {
return;
}
elem.removeAttribute('class');
elem.hide();
};
var _displayTravelersMessage = function (message) {
var elem = _findSelector('#FLIGHTS_MESSAGE_BOX');
if (!elem) {
return;
}
elem.innerHTML = message.message;
elem.addClass(message.type);
elem.show();
};
var _hasInfant = function () {
var childrenNumber = parseInt(_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_CHILDREN_NUMBERS_LABEL).innerHTML, 10);
for (var i = 1; i <= childrenNumber; i++) {
var age = _findSelector('#FLIGHTS_CHILD_AGE_' + i).innerHTML;
if (age === '&lt; 1') {
age = 0;
}
age = parseInt(age, 10);
if (!isNaN(age) && age <= 3) {
return true;
}
}
return false;
};
var _verifyTravelerRules = function () {
var foundError = false;
var adultsNumber = parseInt(_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_ADULTS_NUMBERS_LABEL).innerHTML, 10);
var seniorsNumber = parseInt(_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_SENIORS_NUMBERS_LABEL).innerHTML, 10);
var childrenNumber = parseInt(_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_CHILDREN_NUMBERS_LABEL).innerHTML, 10);
_hideTravelersMessage();
that.widgetElement.getElements('.totalTravelers').removeClass('totalTravelersError');
that.widgetElement.getElements('.adultLabel').removeClass('travelerLabelError');
that.widgetElement.getElements('.adultCounter').removeClass('counterContainerError');
if (adultsNumber + seniorsNumber + childrenNumber === 0) {
that.widgetElement.getElements('.totalTravelers').addClass('totalTravelersError');
foundError = true;
} else if (adultsNumber + seniorsNumber === 0 && childrenNumber > 0) {
_displayTravelersMessage(that.messages.zeroAdults);
that.widgetElement.getElements('.adultLabel').addClass('travelerLabelError');
that.widgetElement.getElements('.adultCounter').addClass('counterContainerError');
foundError = true;
} else if (adultsNumber + seniorsNumber + childrenNumber > 6) {
_displayTravelersMessage(that.messages.overMaxTravelers);
that.widgetElement.getElements('.totalTravelers').addClass('totalTravelersError');
foundError = true;
} else if (adultsNumber + seniorsNumber > 0 && childrenNumber > 0 && _hasInfant()) {
_displayTravelersMessage(that.messages.noInfantInLap);
}
return !foundError;
};
var _checkTravelerErrors = function () {
var rulesOK = _verifyTravelerRules();
var ageOK = _verifyChildrenAges();
if (rulesOK && ageOK) {
_enableSearchButton();
return true;
}
_disableSearchButton();
return false;
};
var _inHierarchy = function (elem, classname) {
while (elem != null) {
elem = ta.id(elem);
if (elem.hasClass(classname)) {
return true;
}
elem = elem.parentElement;
}
return false;
};
var _hideTravelerPopupFromOutside = function (event) {
if (!_inHierarchy(event.target, 'travelersLander') && !_inHierarchy(event.target, 'travelersPopup')) {
_hideTravelerPopup();
}
};
var _hideTravelerPopup = function () {
if (_checkTravelerErrors() && that.travelerPopup) {
ta.id('PAGE').removeEvent('click', _hideTravelerPopupFromOutside);
var popup = _findSelector('#FLIGHTS_TRAVELERS_POPUP');
popup.hide();
popup.removeEvent('click', _verifyTravelerRules);
_findSelector('#FLIGHTS_TRAVELERS_LANDER').setStyle('border-bottom', '');
that.travelerPopup = false;
_enableSearchButton();
}
};
var _showTravelerPopup = function () {
if (!that.travelerPopup) {
var popup = _findSelector('#FLIGHTS_TRAVELERS_POPUP');
popup.show();
popup.addEvent('click', _verifyTravelerRules);
_findSelector('#FLIGHTS_TRAVELERS_LANDER').setStyle('border-bottom', '0');
that.travelerPopup = true;
_disableSearchButton();
ta.id('PAGE').addEvent('click', _hideTravelerPopupFromOutside);
} else {
_hideTravelerPopup();
}
};
var _displayChildAges = function (id, numberOfChildren) {
if (id !== FORM_CONTROL_IDS.FLIGHTS_CHILDREN_NUMBERS) {
return;
}
for (var i = 1; i <= 5; i++) {
_findSelector('#FLIGHTS_AGE_CONTAINER_' + i).setStyle('display', 'none');
}
for (i = 1; i <= numberOfChildren; i++) {
_findSelector('#FLIGHTS_AGE_CONTAINER_' + i).erase('style');
}
if (numberOfChildren === 0) {
_findSelector('#FLIGHTS_CHILDREN_AGE_DISCLAIMER').setStyle('display', 'none');
} else {
_findSelector('#FLIGHTS_CHILDREN_AGE_DISCLAIMER').erase('style');
}
};
var _updateTotalTravelerNumber = function () {
var totalNumber = 0;
_findSelector('#FLIGHTS_TRAVELERS_SELECTOR').getElements('.hiddenTravelerNumber').each(function (elem) {
totalNumber += elem.value.toInt();
});
if (totalNumber < 7) {
_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_TOTAL_TRAVELERS).innerHTML = that.peopleStrings[totalNumber];
} else {
_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_TOTAL_TRAVELERS).innerHTML = that.peopleStrings[7];
}
};
var _increaseTravelerNumber = function (id) {
var elem = _findSelector('#' + id);
if (elem.value < MAX_TRAVELER_NUMBERS[id]) {
elem.value++;
}
_findSelector('#' + id + '_LABEL').innerHTML = elem.value;
_displayChildAges(id, elem.value);
_updateTotalTravelerNumber();
};
var _decreaseTravelerNumber = function (id) {
var elem = _findSelector('#' + id);
if (elem.value > 0) {
elem.value--;
}
_findSelector('#' + id + '_LABEL').innerHTML = elem.value;
_displayChildAges(id, elem.value);
_updateTotalTravelerNumber();
};
var _clearErrors = function () {
_findSelector('#FLIGHTS_SEARCH_FORM').getElements('input.error').removeClass('error');
_findSelector('#FLIGHTS_ERROR' + ' .msg').setContent('');
_findSelector('#FLIGHTS_ERROR').hide();
that.errors = {};
};
var _showError = function (elem, message) {
if (elem && elem.addClass && typeof elem.addClass == 'function') {
elem.addClass('error');
}
if (that.errors[message]) {
return;
}
that.errors[message] = true;
if (that.metaLang[message]) {
message = that.metaLang[message];
var msg = _findSelector('#' + 'FLIGHTS_ERROR' + ' .msg').innerHTML;
if (msg !== '') {
msg += '<br />';
}
msg += message;
_findSelector('#FLIGHTS_ERROR').show();
_findSelector('#' + 'FLIGHTS_ERROR' + ' .msg').set('html', msg);
}
};
var _keepTravelerParams = function () {
var popup = _findSelector('#FLIGHTS_TRAVELERS_POPUP');
if (_canUseLocalStore() && popup) {
try {
localStorage.setItem('travelerInfoTimestamp', new Date().getTime());
localStorage.setItem('adultsNumbers', _findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_ADULTS_NUMBERS).value);
localStorage.setItem('seniorsNumbers', _findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_SENIORS_NUMBERS).value);
localStorage.setItem('childrenNumbers', _findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_CHILDREN_NUMBERS).value);
var ageContainers = popup.getElements('.ageContainer');
for (var i = 0; i < ageContainers.length; i++) {
localStorage.setItem('childAgeSelectIndex' + i, ageContainers[i].getElements('select')[0].selectedIndex);
localStorage.setItem('childAgeValue' + i, ageContainers[i].getElements('.childAgeValue')[0].innerHTML);
localStorage.setItem('childAgeValueHidden' + i, ageContainers[i].getElements('.childAgeValueHidden')[0].value);
}
} catch (e) {
}
}
};
var _restoreAdultTravelers = function () {
var adults = localStorage.getItem('adultsNumbers');
_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_ADULTS_NUMBERS).value = adults;
_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_ADULTS_NUMBERS_LABEL).innerHTML = adults;
};
var _restoreSeniorTravelers = function () {
var seniors = localStorage.getItem('seniorsNumbers');
_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_SENIORS_NUMBERS).value = seniors;
_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_SENIORS_NUMBERS_LABEL).innerHTML = seniors;
};
var _restoreChildTravelers = function (popup) {
var children = localStorage.getItem('childrenNumbers');
_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_CHILDREN_NUMBERS).value = children;
_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_CHILDREN_NUMBERS_LABEL).innerHTML = children;
var ageContainers = popup.getElements('.ageContainer');
for (var i = 0; i < ageContainers.length; i++) {
ageContainers[i].getElements('select')[0].selectedIndex = localStorage.getItem('childAgeSelectIndex' + i);
ageContainers[i].getElements('.childAgeValue')[0].innerHTML = localStorage.getItem('childAgeValue' + i);
ageContainers[i].getElements('.childAgeValueHidden')[0].value = localStorage.getItem('childAgeValueHidden' + i);
}
};
var _restoreTravelerParams = function () {
var popup = _findSelector('#FLIGHTS_TRAVELERS_POPUP');
if (_canUseLocalStore() && popup) {
try {
if (localStorage.getItem('adultsNumbers') !== null) {
_restoreAdultTravelers();
}
if (localStorage.getItem('seniorNumbers') !== null) {
_restoreSeniorTravelers();
}
if (localStorage.getItem('childrenNumbers') !== null) {
_restoreChildTravelers(popup);
}
_updateTotalTravelerNumber();
} catch (e) {
}
}
};
var _formatDateMetaUrl = function (date) {
return (date.getFullYear() * 10000 + ((date.getMonth() + 1) * 100 + date.getDate())).toString();
};
var _adjustForThaiCalendarOffset = function (year) {
if (ta.retrieve('thai_buddhist_era_year') && year >= (new Date().getFullYear() + ta.util.date.THAI_BUDDHIST_YEAR_OFFSET)) {
year -= ta.util.date.THAI_BUDDHIST_YEAR_OFFSET;
}
return year;
};
var _addThaiCalendarOffsetIfNecessary = function (year) {
if (ta.retrieve('thai_buddhist_era_year')) {
_adjustForThaiCalendarOffset(year);
year += ta.util.date.THAI_BUDDHIST_YEAR_OFFSET;
}
return year;
};
var _buildDate = function (month, day) {
var regex = /[\/]/;
var monthYear = _findSelector('#' + month).value.split(regex);
monthYear[1] = _adjustForThaiCalendarOffset(monthYear[1]);
return ta.util.date.create(monthYear[1], monthYear[0] - 1, _findSelector('#' + day).value);
};
var _getTravelersUrlParams = function () {
var urlParams = {};
var count = 0;
var adultsNumber = parseInt(_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_ADULTS_NUMBERS_LABEL).innerHTML, 10);
var seniorsNumber = parseInt(_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_SENIORS_NUMBERS_LABEL).innerHTML, 10);
var childrenNumber = parseInt(_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_CHILDREN_NUMBERS_LABEL).innerHTML, 10);
for (var i = 0; i < adultsNumber; i++) {
urlParams['pax' + count] = 'a';
count++;
}
for (i = 0; i < seniorsNumber; i++) {
urlParams['pax' + count] = 's';
count++;
}
for (i = 1; i <= childrenNumber; i++) {
var age = _findSelector('#' + 'FLIGHTS_CHILD_AGE_' + i).innerHTML;
if (age === '&lt; 1') {
age = 0;
}
urlParams['pax' + count] = age;
count++;
}
urlParams['travelers'] = count;
return urlParams;
};
var _getLastDate = function () {
if (!that.lastDate) {
var endDate = new Date();
ta.util.date.normalize(endDate);
endDate.setTime(endDate.getTime() + 28512000000);
if (ta.retrieve('thai_buddhist_era_year')) {
var dayOfMonth = endDate.getDate();
var month = endDate.getMonth();
var year = _addThaiCalendarOffsetIfNecessary(endDate.getFullYear());
endDate = new Date(year, month, dayOfMonth);
}
that.lastDate = endDate;
}
return that.lastDate;
};
var _buildQueryString = function (servlet, params) {
var url = servlet;
var first = true;
Object.each(params, function (value, key) {
if (!first) {
url += '&';
} else {
first = false;
}
url += key + '=' + value;
});
return url;
};
var _initializePeopleStrings = function () {
var peopleStringsElem = that.widgetElement.getElement('.localizationPeopleStrings');
var nextAttribute = peopleStringsElem.getAttribute('data-people-0');
var i = 1;
while (nextAttribute) {
that.peopleStrings.push(nextAttribute);
nextAttribute = peopleStringsElem.getAttribute('data-people-' + i);
i++;
}
};
var _initializeMessages = function () {
var messagesElem = that.widgetElement.getElement('.localizationMessages');
var infantMessage = messagesElem.getAttribute('data-no-infant-in-lap');
var zeroAdults = messagesElem.getAttribute('data-zero-adults');
var overMaxTravelers = messagesElem.getAttribute('data-over-max-travelers');
that.messages = {
'noInfantInLap': {'message': infantMessage, 'type': 'info'},
'zeroAdults': {'message': zeroAdults, 'type': 'error'},
'overMaxTravelers': {'message': overMaxTravelers, 'type': 'error'}
};
};
var _initializeMetaLang = function () {
var metaLangElem = that.widgetElement.getElement('.localizationMetaLang');
that.metaLang = {
dateFuture: metaLangElem.getAttribute('data-date-future'),
dateRelative: metaLangElem.getAttribute('data-date-relative'),
dateFar: metaLangElem.getAttribute('data-date-far'),
dateMissing: metaLangElem.getAttribute('data-date-missing'),
airport: metaLangElem.getAttribute('data-airport'),
airportsSame: metaLangElem.getAttribute('data-airports-same'),
checkEmail: metaLangElem.getAttribute('data-enter-valid-email')
};
};
var _checkDate = function (status, cal, month, day) {
cal = _findSelector('#' + cal);
var visibleDate = cal.innerHTML;
var date;
var r = visibleDate.match(DATE_FORMAT.pattern);
if (r) {
var nd = parseInt(r[DATE_FORMAT.date].replace(/^0/, ''), 10);
var nm = parseInt(r[DATE_FORMAT.month].replace(/^0/, ''), 10);
var ny = parseInt(r[DATE_FORMAT.year].replace(/^0/, ''), 10);
if (nd > 0 && nd < 32 && nm > 0 && nm < 13) {
if (nd < 10) {
nd = '0' + nd;
}
if (nm < 10) {
nm = '0' + nm;
}
if (ny < 100) {
ny += 2000;
}
_findSelector('#' + day).value = nd;
_findSelector('#' + month).value = nm + "/" + ny;
date = ta.util.date.create(ny, nm - 1, nd);
}
} else {
if (!cal.innerHTML || DATE_FORMAT.pattern.exec(cal.value) === null) {
if (!that.hideErrors) {
_showError(cal, 'dateMissing');
}
status.valid = false;
return;
}
date = _buildDate(month, day);
}
if (date < status.today) {
if (!that.hideErrors) {
_showError(cal, 'dateFuture');
}
status.valid = false;
} else if (status.prev && status.prev > date) {
if (!that.hideErrors) {
_showError(cal, 'dateRelative');
}
status.valid = false;
} else if (date > _getLastDate()) {
if (!that.hideErrors) {
_showError(cal, 'dateFar');
}
status.valid = false;
}
status.prev = date;
};
var _checkAirport = function (status, errId, txtId) {
var airport = _findSelector('#' + txtId).value;
if (!airport || airport.length !== 3) {
if (!that.hideErrors) {
_showError(_findSelector('#' + errId), 'airport');
}
status.valid = false;
}
};
var _checkAirportsSame = function (status, errId1, txtId1, errId2, txtId2) {
var airport1 = _findSelector('#' + txtId1).value;
var airport2 = _findSelector('#' + txtId2).value;
if (airport1 && airport2 && airport1 === airport2) {
if (!that.hideErrors) {
_showError(_findSelector('#' + errId1), 'airportsSame');
_showError(_findSelector('#' + errId2), 'airportsSame');
}
status.valid = false;
}
};
var _checkEmail = function(status) {
var emailNode = _findSelector('#airwatchCheckboxEmail');
var email = emailNode ? emailNode.value : '';
if (!email) {
status.valid = false;
}
var atpos = email.indexOf("@");
var dotpos = email.lastIndexOf(".");
status.valid = atpos > 1 && dotpos > atpos + 2 && dotpos + 2 < email.length;
return status.valid;
};
var _getAdultsAndSeniors = function () {
var adultsNumber = parseInt(_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_ADULTS_NUMBERS).value);
var seniorsNumber = parseInt(_findSelector('#' + FORM_CONTROL_IDS.FLIGHTS_SENIORS_NUMBERS).value);
return adultsNumber + seniorsNumber;
};
var _validateInput = function () {
_clearErrors();
var dToday = new Date();
ta.util.date.normalize(dToday);
return {
prev: null,
today: dToday,
valid: _verifyChildrenAges()
};
};
var _initCardsSearchForms = function() {
var allCardSearchForms = $('.card_searchForm');
allCardSearchForms.each( function(idx, e) {
var itemNo = idx + 1;
$('.locationInfo input', e).each( function(i, ce) {
var idParams = ce.id.split('_');
var offset = itemNo + "" + idParams[1].slice(-1);
ce.id = idParams[0] + "_" + offset + "_" + idParams[2];
});
$('.dateInfo span', e).each( function(i, ce) {
var idParams = ce.id.split('_');
var offset = itemNo + "" + idParams[3].slice(-1);
ce.id = "date_picker" + "_" + idParams[2] + "_" + offset;
});
var sf = $('.searchForm', e);
if ( sf ) {
sf.addClass('cardSearchForm');
}
});
};
this.init = function (widget, searchCallback) {
that.widget = widget;
that.widgetElement = widget.element;
that.searchCallback = searchCallback;
_initializePeopleStrings();
_initializeMessages();
_initializeMetaLang();
};
this.canUseLocalStore = _canUseLocalStore;
this.typeahead = _typeahead;
this.getAdultsAndSeniors = _getAdultsAndSeniors;
this.buildQueryString = _buildQueryString;
this.showError = _showError;
this.validateInput = _validateInput;
this.checkDate = _checkDate;
this.checkAirport = _checkAirport;
this.checkEmail = _checkEmail;
this.checkAirportsSame = _checkAirportsSame;
this.formatDateMetaUrl = _formatDateMetaUrl;
this.addThaiCalendarOffsetIfNecessary = _addThaiCalendarOffsetIfNecessary;
this.buildDate = _buildDate;
this.getTravelersUrlParams = _getTravelersUrlParams;
this.keepTravelerParams = _keepTravelerParams;
this.restoreTravelerParams = _restoreTravelerParams;
this.handleSelectChange = _handleSelectChange;
this.showTravelerPopup = _showTravelerPopup;
this.hideTravelerPopup = _hideTravelerPopup;
this.increaseTravelerNumber = _increaseTravelerNumber;
this.decreaseTravelerNumber = _decreaseTravelerNumber;
this.initCardsSearchForms = _initCardsSearchForms;
};
return {
IDS: FORM_CONTROL_IDS,
TRIP_TYPES: TRIP_TYPES,
FlightSearchFormController: FlightSearchFormController
};
});
require(['ta/prwidgets'], function(widgets) {
var define = widgets.define.bind(widgets,'flights_lander_search_form','handlers',['handlers']);
define(["widget", "ta", 'flights/flight_search_results/FlightSearchForm'], function (widget, ta, FlightSearchForm) {
var m_fltSearchForm = new FlightSearchForm.FlightSearchFormController();
var m_widgetElement = widget.element;
var TRIP_TYPES = FlightSearchForm.TRIP_TYPES;
var m_prevTripType = TRIP_TYPES.ROUND_TRIP;
var m_tripType = TRIP_TYPES.ROUND_TRIP;
if (ta.retrieve('oneWayView')) { // set on dust model
m_tripType = TRIP_TYPES.ONE_WAY;
} else if (ta.retrieve('multiCityView')) {
m_tripType = TRIP_TYPES.MULTI_CITY;
}
var m_canUseSessionStore = ta.util.sessionStorage && ta.util.sessionStorage.canUseSessionStore();
var m_isLanderPage = !ta.retrieve('isSearchResultsPage');
var TRIP_TYPE_SELECTION_KEY = 'lander_trip_type_selection'; // used to store & retrieve tripType selection from sessionStorage
if (m_canUseSessionStore
&& m_isLanderPage
&& sessionStorage.getItem(TRIP_TYPE_SELECTION_KEY)) {
m_tripType = sessionStorage.getItem(TRIP_TYPE_SELECTION_KEY); // override with value stored in the sessionStorage
}
var m_active = false;
var m_hideErrors = false;
var m_moreOptionsOpen = ta.retrieve('moreOptionsView');
var AIRWATCH_PID = 39857;
var _findSelector = function (selector) {
return m_widgetElement.querySelector(selector);
};
var _getSearchForm = function (tripType) {
if (tripType === TRIP_TYPES.MULTI_CITY) {
return _findSelector('#FLIGHTS_MC_SEARCH_FORM');
}
if (tripType === TRIP_TYPES.ONE_WAY) {
return _findSelector('#FLIGHTS_OW_SEARCH_FORM');
}
return _findSelector('#FLIGHTS_RT_SEARCH_FORM');
};
var _getFirstCalIndex = function (tripType) {
var searchForm = _getSearchForm(tripType);
if (searchForm == null) {
return 0;
}
var datePickers = searchForm.querySelectorAll('.date_picker');
if (datePickers == null || datePickers.length === 0) {
return 0;
}
return parseInt(datePickers[0].id.replace('date_picker_in_', '').replace('date_picker_out_', ''));
};
var _getFirstTypeaheadIndex = function (tripType) {
var searchForm = _getSearchForm(tripType);
if (searchForm == null) {
return 0;
}
var fromTypeaheads = searchForm.querySelectorAll("[id$='airportFrom']");
if (fromTypeaheads == null || fromTypeaheads.length === 0) {
return 0;
}
//The index will be the second split element of typeahead's ID
var idParams = fromTypeaheads[0].id.split("_");
return parseInt(idParams[1]);
};
/**
* Returns the prefix for type of search, used to identify specific
* element ids.
*/
var _getTripTypePrefix = function () {
if (m_tripType === TRIP_TYPES.ONE_WAY) {
return 'ow_'
} else if (m_tripType === TRIP_TYPES.MULTI_CITY) {
return 'mc_'
} else {
return 'rt_';
}
};
/**
* Check if the dates in the multi city fields are not overlapping
*/
var _checkMultiCityDates = function (startField, d) {
//Make sure none of the fields after this one are chronologically before it
//Check all of the date_picker objects after startField and reset them if their date is before this date
var maxDatePickers = 4;
var mcCalOffset = _getFirstCalIndex(TRIP_TYPES.MULTI_CITY);
for (var i = startField; i < maxDatePickers + mcCalOffset; i++) {
if (_findSelector('#date_picker_in_' + i) && _findSelector('#date_picker_in_' + i).innerHTML.length > 1) {
var otherDate = ta.widgets.calendar.dateFromString(_findSelector('#date_picker_in_' + i).innerHTML);
if (d > otherDate) {
_findSelector('#date_picker_in_' + i).innerHTML = "";
}
}
}
};
/**
* Helper method that assigns the given value to an array of elements passed in.
*/
var _assignElemValues = function (elems, value) {
elems.forEach(function (e) {
_findSelector('#' + e).value = value;
if (_findSelector('#' + e).hasClass('ghostText')) {
_findSelector('#' + e).removeClass('ghostText');
}
});
};
/**
* Sync the date picker 'in' dates through all trip types
*/
var _syncInDates = function () {
if (m_prevTripType === m_tripType) {
return;
}
var rtOffset = _getFirstCalIndex(TRIP_TYPES.ROUND_TRIP);
var owOffset = _getFirstCalIndex(TRIP_TYPES.ONE_WAY);
var mcOffset = _getFirstCalIndex(TRIP_TYPES.MULTI_CITY);
var rtDatePicker = _findSelector('#date_picker_in_' + rtOffset);
var owDatePicker = _findSelector('#date_picker_in_' + owOffset);
var mcDatePicker = _findSelector('#date_picker_in_' + mcOffset);
if (rtDatePicker == null || owDatePicker == null || mcDatePicker == null) {
return;
}
var inDate;
if (m_prevTripType === TRIP_TYPES.ROUND_TRIP) {
owDatePicker.innerHTML = rtDatePicker.innerHTML;
mcDatePicker.innerHTML = rtDatePicker.innerHTML;
inDate = ta.widgets.calendar.dateFromString(rtDatePicker.innerHTML);
_checkMultiCityDates(mcOffset, inDate);
_assignElemValues(['ow_leaveDay', 'mc_0_leaveDay'], _findSelector('#rt_leaveDay').value);
_assignElemValues(['ow_leaveMonth', 'mc_0_leaveMonth'], _findSelector('#rt_leaveMonth').value);
} else if (m_prevTripType === TRIP_TYPES.ONE_WAY) {
rtDatePicker.innerHTML = owDatePicker.innerHTML;
mcDatePicker.innerHTML = owDatePicker.innerHTML;
inDate = ta.widgets.calendar.dateFromString(owDatePicker.innerHTML);
_checkMultiCityDates(mcOffset, inDate);
_assignElemValues(['rt_leaveDay', 'mc_0_leaveDay'], _findSelector('#ow_leaveDay').value);
_assignElemValues(['rt_leaveMonth', 'mc_0_leaveMonth'], _findSelector('#ow_leaveMonth').value);
} else {
rtDatePicker.innerHTML = mcDatePicker.innerHTML;
owDatePicker.innerHTML = mcDatePicker.innerHTML;
inDate = ta.widgets.calendar.dateFromString(mcDatePicker.innerHTML);
_assignElemValues(['ow_leaveDay', 'rt_leaveDay'], _findSelector('#mc_0_leaveDay').value);
_assignElemValues(['ow_leaveMonth', 'rt_leaveMonth'], _findSelector('#mc_0_leaveMonth').value);
}
m_prevTripType = m_tripType;
if (!inDate) {
return;
}
ta.util.date.normalize(inDate);
ta.widgets.calendar.setPageInDate(inDate);
};
/**
* Restore multi city value.
*/
var _restoreMultiCityFields = function () {
if (!m_fltSearchForm.canUseLocalStore()) {
return;
}
var mcTypeaheadOffset = _getFirstTypeaheadIndex(TRIP_TYPES.MULTI_CITY);
var mcCalOffset = _getFirstCalIndex(TRIP_TYPES.MULTI_CITY);
for (var i = 0; i < 4; i++) {
var mcprefix = _getTripTypePrefix() + (i + mcTypeaheadOffset) + '_';
if (localStorage.getItem('multiCity-from-row-' + i)) {
var airportFrom = localStorage.getItem('multiCity-from-row-' + i).split('|');
if (airportFrom.length !== 2) {
continue;
}
_assignElemValues([mcprefix + 'airportFromCode'], airportFrom[0]);
_assignElemValues([mcprefix + 'airportFrom'], airportFrom[1]);
}
if (localStorage.getItem('multiCity-to-row-' + i)) {
var airportTo = localStorage.getItem('multiCity-to-row-' + i).split('|');
if (airportTo.length !== 2) {
continue;
}
_assignElemValues([mcprefix + 'airportToCode'], airportTo[0]);
_assignElemValues([mcprefix + 'airportTo'], airportTo[1]);
}
if (localStorage.getItem('multiCity-date' + i) && _findSelector('#date_picker_in_' + (i + mcCalOffset)).innerHTML === ''
&& _findSelector('#' + mcprefix + 'airportFromCode').value !== '' && _findSelector('#' + mcprefix + 'airportToCode').value !== '') {
var date = localStorage.getItem('multiCity-date' + i);
_findSelector('#date_picker_in_' + (i + mcCalOffset)).innerHTML = date;
_findSelector('#mc_' + i + '_leaveDay').value = localStorage.getItem('multiCity-day' + i);
_findSelector('#mc_' + i + '_leaveMonth').value = localStorage.getItem('multiCity-month' + i);
_checkMultiCityDates(i + mcCalOffset, date);
}
}
};
var _isVisible = function (elem) {
if (typeof(elem) === "string" || elem instanceof String) {
elem = _findSelector(elem) || _findSelector("#" + elem);
}
return elem && elem.offsetWidth > 0 && elem.offsetHeight > 0;
};
/**
* Toggles flexible link state based on airport selected.
*/
var _updateFlexibleLink = function () {
if (!_isVisible('more-options')) {
return;
}
var cabin = parseInt(_findSelector('#cabin').value);
//Hide the fare calendar link when trip is not roundrip or cabin is not economy
if (m_tripType !== TRIP_TYPES.ROUND_TRIP ||  cabin !== 0) {
_findSelector('#flexibleLinkContainer').hide();
return;
}
var rtTypeaheadOffset = _getFirstTypeaheadIndex(TRIP_TYPES.ROUND_TRIP);
var numChildren = parseInt(_findSelector('#' + FlightSearchForm.IDS.FLIGHTS_CHILDREN_NUMBERS).value);
if (_findSelector('#rt_' + rtTypeaheadOffset + '_airportFromCode').value.match(/[A-Z]{3}/) &&
_findSelector('#rt_' + rtTypeaheadOffset + '_airportToCode').value.match(/[A-Z]{3}/) &&
numChildren === 0 && cabin === 0) {
_findSelector('#flexibleLinkContainer').style.display = 'inline-block';
}
};
/**
* Expands the more options section.
*/
var _showMoreOptions = function () {
if (m_moreOptionsOpen === false) {
ta.trackEventOnPage('FL_Search_Form_Large', 'FL_SF_MoreOptions', '', 0, false);
}
_findSelector('#mo-link-line').hide();
_findSelector('#more-options').show();
m_moreOptionsOpen = true;
_updateFlexibleLink();
};
var _selectAll = function () {
var providersInputId = _findSelector('#PROVIDERS').getElementsByTagName('input');
if (typeof providersInputId !== 'undefined' && providersInputId.length > 0) {
for (var i = 0; i < providersInputId.length; i++) {
if (!providersInputId[i].checked) {
providersInputId[i].checked = true;
}
}
}
};
/**
* Checked if user is susbribed to the give o-d route for airwatch alerts.
*/
var _isSubscribedToRoute = function (origin, dest) {
var airwatchPairs = ta.retrieve('airwatch-pairs');
origin = origin.toUpperCase().trim();
dest = dest.toUpperCase().trim();
var pair = origin + '-' + dest;
if (m_fltSearchForm.canUseLocalStore() && localStorage.getItem('airwatch_signup.' + origin + dest)) {
return true;
}
return airwatchPairs.indexOf(pair) > -1;
};
/**
* Sets airwatch block state and returns a boolean value
* if airwatch is visible or not.
*/
var _updateInlineAirwatch = function (from, to) {
if (!_findSelector('#airwatch')) {
return;
}
var fromServlet = 'Flights';
var airwatch = _findSelector('#airwatch');
if (ta.has('flights.referringServletName')) {
fromServlet = ta.retrieve('flights.referringServletName');
}
new Request.JSON({
url: "/FlightsController?from=" + fromServlet + "&AW=true&Orig=" + from + "&Dest=" + to,
async: false,
onSuccess: function (response) {
if (!response.showAwSignup) {
_findSelector('#airwatchCheckbox').checked = false;
airwatch.addClass('unsupported-route');
} else {
airwatch.removeClass('unsupported-route');
}
}
}).send();
if (_isSubscribedToRoute(from, to)) {
airwatch.addClass('subscribed', _isSubscribedToRoute(from, to));
} else {
airwatch.removeClass('subscribed', _isSubscribedToRoute(from, to));
}
};
/**
* Get the Geo Code of the lander.
*/
var _getGeoCode = function () {
var geos = /g(\d+)/.exec(window.location);
if (geos) {
return geos[1];
} else {
return 1;
}
};
/**
* Updating the list of providers for the given o-d pair
*
* @param isLbPageview: Whether this update should be recorded as a new pageview in the lookback log
*/
var _updateProviders = function (isLbPageview) {
var pcParam = '&pc=true';
var isRT = m_tripType === TRIP_TYPES.ROUND_TRIP ? 'yes' : 'no';
var fromServlet = 'Flights';
if (ta.has('flights.referringServletName')) {
fromServlet = ta.retrieve('flights.referringServletName');
}
var typeaheadPrefix = '#' + _getTripTypePrefix() + _getFirstTypeaheadIndex(m_tripType);
var url = "/FlightsController?from="
+ fromServlet + "&geo=" + _getGeoCode()
+ "&Orig=" + _findSelector(typeaheadPrefix + '_airportFromCode').value
+ "&Dest=" + _findSelector(typeaheadPrefix + '_airportToCode').value
+ '&roundtrip=' + isRT;
var providers = _findSelector('#PROVIDERS');
if (providers) {
// Perform AJAX request
var lbParams = '';
if (isLbPageview) {
lbParams = "&lbLogType=AJAX&lbServletName=" + fromServlet;
}
new Request.HTML({
url: url + pcParam + lbParams,
update: providers,
evalScripts: true,
onFailure: function (xhr) {
if (xhr.status === 404) {
providers.set('html', '<div></div>');
}
}
}).send();
}
var fdProviders = _findSelector('#FD_PROVIDERS');
if (fdProviders) {
var fdUrl = "/FlightsController?from=" + fromServlet + "&FD=True&" + url;
if (ta && ta.retrieve) {
fdUrl += '&puid=' + ta.retrieve('flights.puid');
}
// Perform AJAX request
new Request.HTML({
url: fdUrl,
update: fdProviders,
evalScripts: true,
onFailure: function (xhr) {
if (xhr.status === 404) {
fdProviders.set('html', '<div></div>');
}
}
}).send();
}
};
/**
* Syncs up the o-d values between the three trip types.
*/
var _syncODValues = function () {
var rtTypeaheadIndex = _getFirstTypeaheadIndex(TRIP_TYPES.ROUND_TRIP);
var owTypeaheadIndex = _getFirstTypeaheadIndex(TRIP_TYPES.ONE_WAY);
var mcTypeaheadIndex = _getFirstTypeaheadIndex(TRIP_TYPES.MULTI_CITY);
var typeaheadIndex = {'roundTrip': rtTypeaheadIndex, 'oneWay': owTypeaheadIndex, 'multiCity': mcTypeaheadIndex};
if (m_tripType !== TRIP_TYPES.MULTI_CITY) {
if (_findSelector('#' + _getTripTypePrefix() + typeaheadIndex[m_tripType] + '_airportFromCode').value.match(/[A-Z]{3}/)) {
if (m_tripType === TRIP_TYPES.ONE_WAY) {
_assignElemValues(['rt_' + typeaheadIndex.roundTrip + '_airportFromCode', 'mc_' + typeaheadIndex.multiCity + '_airportFromCode'], _findSelector('#ow_' + typeaheadIndex.oneWay + '_airportFromCode').value);
_assignElemValues(['rt_' + typeaheadIndex.roundTrip + '_airportFrom', 'mc_' + typeaheadIndex.multiCity + '_airportFrom'], _findSelector('#ow_' + typeaheadIndex.oneWay + '_airportFrom').value);
} else {
_assignElemValues(['ow_' + typeaheadIndex.oneWay + '_airportFromCode', 'mc_' + typeaheadIndex.multiCity + '_airportFromCode'], _findSelector('#rt_' + typeaheadIndex.roundTrip + '_airportFromCode').value);
_assignElemValues(['ow_' + typeaheadIndex.oneWay + '_airportFrom', 'mc_' + typeaheadIndex.multiCity + '_airportFrom'], _findSelector('#rt_' + typeaheadIndex.roundTrip + '_airportFrom').value);
}
}
if (_findSelector('#' + _getTripTypePrefix() + typeaheadIndex[m_tripType] + '_airportToCode').value.match(/[A-Z]{3}/)) {
if (m_tripType === TRIP_TYPES.ONE_WAY) {
_assignElemValues(['rt_' + typeaheadIndex.roundTrip + '_airportToCode', 'mc_' + typeaheadIndex.multiCity + '_airportToCode'], _findSelector('#ow_' + typeaheadIndex.oneWay + '_airportToCode').value);
_assignElemValues(['rt_' + typeaheadIndex.roundTrip + '_airportTo', 'mc_' + typeaheadIndex.multiCity + '_airportTo'], _findSelector('#ow_' + typeaheadIndex.oneWay + '_airportTo').value);
} else {
_assignElemValues(['ow_' + typeaheadIndex.oneWay + '_airportToCode', 'mc_' + typeaheadIndex.multiCity + '_airportToCode'], _findSelector('#rt_' + typeaheadIndex.roundTrip + '_airportToCode').value);
_assignElemValues(['ow_' + typeaheadIndex.oneWay + '_airportTo', 'mc_' + typeaheadIndex.multiCity + '_airportTo'], _findSelector('#rt_' + typeaheadIndex.roundTrip + '_airportTo').value);
}
}
} else {
if (_findSelector('#' + _getTripTypePrefix() + typeaheadIndex[m_tripType] + '_airportFromCode').value.match(/[A-Z]{3}/)) {
_assignElemValues(['rt_' + typeaheadIndex.roundTrip + '_airportFromCode', 'ow_' + typeaheadIndex.oneWay + '_airportFromCode'], _findSelector('#mc_' + typeaheadIndex.multiCity + '_airportFromCode').value);
_assignElemValues(['rt_' + typeaheadIndex.roundTrip + '_airportFrom', 'ow_' + typeaheadIndex.oneWay + '_airportFrom'], _findSelector('#mc_' + typeaheadIndex.multiCity + '_airportFrom').value);
}
if (_findSelector('#' + _getTripTypePrefix() + typeaheadIndex[m_tripType] + '_airportToCode').value.match(/[A-Z]{3}/)) {
_assignElemValues(['rt_' + typeaheadIndex.roundTrip + '_airportToCode', 'ow_' + typeaheadIndex.oneWay + '_airportToCode'], _findSelector('#mc_' + typeaheadIndex.multiCity + '_airportToCode').value);
_assignElemValues(['rt_' + typeaheadIndex.roundTrip + '_airportTo', 'ow_' + typeaheadIndex.oneWay + '_airportTo'], _findSelector('#mc_' + typeaheadIndex.multiCity + '_airportTo').value);
}
}
};
/**
* Toggles the providers and airwatch container block state based on airport selected.
*
* @param isLbPageview: Whether this update should be recorded as a new pageview in the lookback log
*/
var _updateAirport = function (isLbPageview) {
var airwatch = _findSelector('#airwatch');
var typeaheadIndex = _getFirstTypeaheadIndex(m_tripType);
if ((m_tripType !== TRIP_TYPES.MULTI_CITY) &&
_findSelector('#' + _getTripTypePrefix() + typeaheadIndex + '_airportFromCode').value.match(/[A-Z]{3}/) &&
_findSelector('#' + _getTripTypePrefix() + typeaheadIndex + '_airportToCode').value.match(/[A-Z]{3}/)) {
if (m_tripType === TRIP_TYPES.ONE_WAY) {
if (airwatch) {
airwatch.hide();
}
} else {
_updateInlineAirwatch(_findSelector('#rt_' + typeaheadIndex + '_airportFromCode').value, _findSelector('#rt_' + typeaheadIndex + '_airportToCode').value);
if (airwatch) {
var showAirwatch = airwatch.hasClass('subscribed') || airwatch.hasClass('unsupported-route') ? false : true;
if (showAirwatch) {
airwatch.show();
} else {
airwatch.hide();
}
}
}
_updateProviders(isLbPageview);
_findSelector('#PROVIDERS').show();
} else {
_findSelector('#PROVIDERS').hide();
if (!airwatch) {
return;
}
if (m_tripType !== TRIP_TYPES.ROUND_TRIP) {
airwatch.hide();
} else {
airwatch.show();
}
}
};
/**
* Sets search type based on user selection and corresponding UI toggles.
*/
var _setSearchType = function (tripType, event) {
if (event && event.preventDefault) {
event.preventDefault();
}
m_tripType = tripType;
// remove 'active' from all tabs
ta.select(m_widgetElement, ".tab").forEach(function(tab) {
if(tab.hasClass(tripType)) {
tab.addClass('active');
} else if(tab.hasClass('active')) {
tab.removeClass('active');
}
});
// remove 'active' from all tabContent
ta.select(m_widgetElement, ".tabContent").forEach(function(tabContent) {
if(tabContent.id === tripType) {
tabContent.addClass('active');
} else if(tabContent.hasClass('active')) {
tabContent.removeClass('active');
}
});
var moreLinkLine = _findSelector('#mo-link-line');
var moreOptions = _findSelector('#more-options');
var travelersWidget = _findSelector('#travelers-widget');
var buttonContainer = _findSelector('#buttonContainer');
//Multi city search specific options
if (m_tripType === TRIP_TYPES.MULTI_CITY) {
moreLinkLine.hide();
moreOptions.hide();
travelersWidget.addClass('multicity');
if (buttonContainer.hasClass('rtow')) {
buttonContainer.removeClass('rtow');
}
buttonContainer.addClass('multicity');
_restoreMultiCityFields();
} else {
if (travelersWidget.hasClass('multicity'))
travelersWidget.removeClass('multicity');
if (buttonContainer.hasClass('multicity'))
buttonContainer.removeClass('multicity');
buttonContainer.addClass('rtow');
if (m_moreOptionsOpen) {
_showMoreOptions();
}
if (!_isVisible(moreLinkLine) && !_isVisible(moreOptions)) {
moreLinkLine.show();
}
}
_syncInDates();
var rtCalOffset = _getFirstCalIndex(TRIP_TYPES.ROUND_TRIP);
if (m_tripType === TRIP_TYPES.ROUND_TRIP) {
var retDate = ta.widgets.calendar.getPageOutDate();
if (retDate) {
var dateFormat = ta.widgets.calendar.getDatePickerFormat('classic');
_findSelector('#date_picker_out_' + rtCalOffset).innerHTML = ta.widgets.calendar.dateToString(retDate, dateFormat);
_findSelector('#rt_retDay').value = retDate.getDate();
_findSelector('#rt_retMonth').value = (retDate.getMonth() + 1) + '/'
+ m_fltSearchForm.addThaiCalendarOffsetIfNecessary(retDate.getFullYear());
}
} else {
//Attempt to set page out-date from date-picker fields if not switching from MC
var outDate = ta.widgets.calendar.dateFromString(_findSelector('#date_picker_out_' + rtCalOffset).innerHTML);
ta.util.date.normalize(outDate);
ta.widgets.calendar.setPageOutDate(outDate);
}
//If valid o-d airport values in form update providers & aw where applicable
_updateAirport(true);
//Flexible link can only be shown for round trip
_updateFlexibleLink();
// Persist tritype selection on lander page only
if (m_canUseSessionStore) { // store tab selection in the sessionStorage
sessionStorage.setItem(TRIP_TYPE_SELECTION_KEY, m_tripType);
}
};
/**
* Called from the typeahead factory on selecting
* a value from the autocompleter list.
*/
var _typeaheadSelect = function (elem, resObj) {
elem.value = resObj.name;
_findSelector('#' + elem.id + 'Code').value = resObj.value;
if (elem.value !== elem.previousValue) {
elem.previousValue = elem.value;
_updateFlexibleLink();
_updateAirport(true);
_syncODValues();
// Hide the text that includes city names, because we don't know the new ones
if (_findSelector('#airwatch')) {
_findSelector('#airwatchCheckboxCityText').hide();
_findSelector('#airwatchCheckboxNoCityText').setStyle('display', 'inline');
}
}
};
var _typeahead = function (event, elem) {
m_fltSearchForm.typeahead(event, elem, _typeaheadSelect, m_tripType);
};
/**
* Event handler for when the class of service is selected.
*/
var _cabinSelectChange = function (elem) {
ta.trackEventOnPage('FL_Search_Form_Large', 'FL_SF_ClassofService', '', 0, false);
if (m_fltSearchForm.canUseLocalStore()) {
localStorage.setItem("session-flight-cabin-value", elem.value);
}
if (m_tripType === TRIP_TYPES.MULTI_CITY) {
_findSelector("#cabin").value = elem.value;
return;
} else {
_findSelector("#mc_cabin").value = elem.value;
}
_updateFlexibleLink();
};
/**
* Event handler for when the travel date is selected.
*/
var _pickdate = function (event, elem, ret) {
var crdId = parseInt((elem.id).replace('date_picker_in_', '').replace('date_picker_out_', ''));
var options = {flights: true};
if (ret) {
//the name out refers to 'Checkout'. In flight terms its return flight,typically only set for return flights on round trip.
ta.store('ta.calendar.isOutDateTrigger', true);
}
if (m_tripType === TRIP_TYPES.MULTI_CITY) {
options.multiCityFlight = true;
}
else if (m_tripType === TRIP_TYPES.ONE_WAY) {
options.oneWayFlight = true;
}
ta.call('ta.overlays.Factory.loadDatePicker', event, elem, crdId, false, ta.widgets.calendar.justUpdateSessionDates, options);
};
/**
* Event handler for when the airwatch checkbox is checked.
*/
var _onAirWatchCheckbox = function (event, isSignedIn) {
event = event || window.event;
if (event.stopPropagation) {
event.stopPropagation();
}
if (isSignedIn) {
return;
}
var isStreamlinedAW = ta.retrieve('airm.isStreamlinedAirwatch');
var isAWChecked = _findSelector('#airwatchCheckbox').checked;
var emailElement = _findSelector('#airwatchCheckboxEmail');
var emailElementCheck = isAWChecked ? emailElement && emailElement.show : emailElement && emailElement.hide;
if (!isStreamlinedAW || !emailElementCheck) {
return;
}
if (isAWChecked) {
emailElement.style.display = 'inline-block';
if (!isStreamlinedAW) {
ta.store('suppressAWCheapFlights');
}
} else {
emailElement.hide();
}
};
/**
* Validate all the input values before the search.
*/
var _validateInput = function (farecal) {
var status = m_fltSearchForm.validateInput();
//Checking the input values
var prefix = _getTripTypePrefix();
var cnt = 0;
var calOffset = _getFirstCalIndex(m_tripType);
var typeaheadOffset = _getFirstTypeaheadIndex(m_tripType);
if (m_tripType === TRIP_TYPES.MULTI_CITY) {
for (var i = 0; i < 4; i++) {
var taprefix = 'mc_' + (i + typeaheadOffset) + '_';
var dateFieldPrefix = 'mc_' + i + '_';
if (_findSelector('#' + taprefix + 'airportFromCode').value !== '' || _findSelector('#' + taprefix + 'airportToCode').value !== '') {
m_fltSearchForm.checkDate(status, 'date_picker_in_' + parseInt(i + calOffset), dateFieldPrefix + 'leaveMonth', dateFieldPrefix + 'leaveDay');
m_fltSearchForm.checkAirport(status, taprefix + 'airportFrom', taprefix + 'airportFromCode');
m_fltSearchForm.checkAirport(status, taprefix + 'airportTo', taprefix + 'airportToCode');
m_fltSearchForm.checkAirportsSame(status, taprefix + 'airportFrom', taprefix + 'airportFromCode', taprefix + 'airportTo', taprefix + 'airportToCode');
cnt++;
}
}
if (cnt === 0) {
// all blank, show errors on first row
m_fltSearchForm.checkDate(status, 'date_picker_in_' + calOffset, 'mc_0_leaveMonth', 'mc_0_leaveDay');
m_fltSearchForm.checkAirport(status, 'mc_' + typeaheadOffset + '_airportFrom', 'mc_' + typeaheadOffset + '_airportFromCode');
m_fltSearchForm.checkAirport(status, 'mc_' + typeaheadOffset + '_airportTo', 'mc_' + typeaheadOffset + '_airportToCode');
}
} else {
m_fltSearchForm.checkAirport(status, prefix + typeaheadOffset + '_airportFrom', prefix + typeaheadOffset + '_airportFromCode');
m_fltSearchForm.checkAirport(status, prefix + typeaheadOffset + '_airportTo', prefix + typeaheadOffset + '_airportToCode');
m_fltSearchForm.checkAirportsSame(status, prefix + typeaheadOffset + '_airportFrom', prefix + typeaheadOffset + '_airportFromCode', prefix + typeaheadOffset + '_airportTo', prefix + typeaheadOffset + '_airportToCode');
if (!farecal) {
m_fltSearchForm.checkDate(status, 'date_picker_in_' + calOffset, prefix + 'leaveMonth', prefix + 'leaveDay');
if (m_tripType === TRIP_TYPES.ROUND_TRIP) {
m_fltSearchForm.checkDate(status, 'date_picker_out_' + calOffset, prefix + 'retMonth', prefix + 'retDay');
}
}
}
if (m_tripType == TRIP_TYPES.ROUND_TRIP && ta.retrieve('airm.isStreamlinedAirwatch') && !ta.retrieve('isLoggedIn') && _findSelector('#airwatchCheckbox') && _findSelector('#airwatchCheckbox').checked) {
if (!m_fltSearchForm.checkEmail(status) && !m_hideErrors) {
m_fltSearchForm.showError(_findSelector('#airwatchCheckboxEmail'), 'checkEmail'); //Enter valid email address.
ta.trackEventOnPage(ta.retrieve('searchFormLoadServlet'), 'FL_temp_AirWatch_InvalidEmail', '', AIRWATCH_PID, true);
}
}
return status;
};
/**
* Retrieves an object of all the airport url parameters.
*/
var _getAirportUrlParams = function () {
var urlParams = {};
var count = 0;
var tripTypePrefix = _getTripTypePrefix();
var typeaheadOffset = _getFirstTypeaheadIndex(m_tripType);
if (m_tripType !== TRIP_TYPES.MULTI_CITY) {
urlParams['airport0'] = _findSelector('#' + tripTypePrefix + typeaheadOffset + '_airportFromCode').value;
urlParams['airport1'] = _findSelector('#' + tripTypePrefix + typeaheadOffset + '_airportToCode').value;
return urlParams;
}
var calOffset = _getFirstCalIndex(TRIP_TYPES.MULTI_CITY);
for (var i = 0; i < 4; i++) {
var mcprefix = tripTypePrefix + (i + typeaheadOffset) + '_';
if (_findSelector('#' + mcprefix + 'airportFromCode').value !== '') {
urlParams['airport' + count] = _findSelector('#' + mcprefix + 'airportFromCode').value;
if (m_fltSearchForm.canUseLocalStore() && i > 0) {
localStorage.setItem('multiCity-from-row-' + i, _findSelector('#' + mcprefix + 'airportFromCode').value + '|' + _findSelector('#' + mcprefix + 'airportFrom').value);
}
count++;
}
if (_findSelector('#' + mcprefix + 'airportToCode').value !== '') {
urlParams['airport' + count] = _findSelector('#' + mcprefix + 'airportToCode').value;
if (m_fltSearchForm.canUseLocalStore() && i > 0) {
localStorage.setItem('multiCity-to-row-' + i, _findSelector('#' + mcprefix + 'airportToCode').value + '|' + _findSelector('#' + mcprefix + 'airportTo').value);
}
count++;
}
var crId = i + calOffset;
if (crId > calOffset && _findSelector('#date_picker_in_' + crId).innerHTML !== '' && m_fltSearchForm.canUseLocalStore()) {
localStorage.setItem('multiCity-date' + i, _findSelector('#date_picker_in_' + crId).innerHTML);
localStorage.setItem('multiCity-day' + i, _findSelector('#mc_' + i + '_leaveDay').value);
localStorage.setItem('multiCity-month' + i, _findSelector('#mc_' + i + '_leaveMonth').value);
}
}
return urlParams;
};
/**
* Retrieves an object of all the dates url parameters.
*/
var _getDatesUrlParams = function (numAirportPairs) {
var urlParams = {};
var tripTypePrefix = _getTripTypePrefix();
if (m_tripType !== TRIP_TYPES.MULTI_CITY) {
urlParams['date0'] = m_fltSearchForm.formatDateMetaUrl(m_fltSearchForm.buildDate(tripTypePrefix + 'leaveMonth', tripTypePrefix + 'leaveDay'));
if (m_tripType !== TRIP_TYPES.ONE_WAY)
urlParams['date1'] = m_fltSearchForm.formatDateMetaUrl(m_fltSearchForm.buildDate(tripTypePrefix + 'retMonth', tripTypePrefix + 'retDay'));
} else {
for (var i = 0; i < numAirportPairs; i++) {
var mcprefix = tripTypePrefix + i + '_';
urlParams['date' + i] = m_fltSearchForm.formatDateMetaUrl(m_fltSearchForm.buildDate(mcprefix + 'leaveMonth', mcprefix + 'leaveDay'));
}
}
return urlParams;
};
/**
* Returns the providers parameters for the /FlightsComparePartners URL.
*/
var _providersUrl = function (prefix) {
var provCnt = 0;
var nepp = _findSelector('#PROVIDERS').getElementsByTagName('input');
var out = '';
for (var i = 0; i < nepp.length; i++) {
var neppi = nepp[i];
if (neppi.checked) {
var nm = neppi.name.replace(prefix, '');
out += "&provider" + provCnt + "=" + nm;
provCnt++;
}
}
return out;
};
/**
* Retrieves the URL for the /FlightsComparePartner window based on the providers checked.
*/
var _getProvidersWindowUrl = function () {
var pfx = _getTripTypePrefix();
var tpfx = _getFirstTypeaheadIndex(m_tripType);
var lcb_url = false;
if (ta.retrieve("compare_partners_enabled") && m_tripType !== TRIP_TYPES.MULTI_CITY) {
lcb_url = "/FlightsComparePartners";
lcb_url += "?airport0=" + _findSelector('#' + pfx + tpfx + '_airportFromCode').value +
"&airport1=" + _findSelector('#' + pfx + tpfx + '_airportToCode').value +
"&date0=" + m_fltSearchForm.formatDateMetaUrl(m_fltSearchForm.buildDate(pfx + 'leaveMonth', pfx + 'leaveDay'));
if (m_tripType === TRIP_TYPES.ROUND_TRIP) {
lcb_url += "&date1=" + m_fltSearchForm.formatDateMetaUrl(m_fltSearchForm.buildDate(pfx + 'retMonth', pfx + 'retDay'));
}
lcb_url += "&" + m_fltSearchForm.buildQueryString('', m_fltSearchForm.getTravelersUrlParams());
// the following parameters aren't checked by the FlightsComparePartners servlet, but included for TA search link on that page
var includeNearby = _findSelector('#includeNearby');
var ns = _findSelector('#nonstopOnly');
lcb_url += "&cos=" + _findSelector('#cabin').value +
"&nonstop=" + ((ns && ns.checked) ? 'yes' : 'no') +
"&nearby0=" + ((includeNearby && includeNearby.checked) ? 'yes' : 'no') +
"&nearby1=" + ((includeNearby && includeNearby.checked) ? 'yes' : 'no') +
"&time0=0024&time1=0024";
var providersUrl = _providersUrl("cb_");
if (!providersUrl) {
return false;
}
lcb_url += providersUrl;
if (ta.has('flights.referringServletName')) {
lcb_url += "&from=" + ta.retrieve('flights.referringServletName');
}
}
return lcb_url;
};
/**
* Returns the airwatch Ajax Url.
*/
var _getAwAjaxUrl = function (useHTTPS) {
var servlet = '/AirWatchSubscriptionAjax';
return (useHTTPS ? ta.util.URL.https(servlet) : servlet);
};
/**
* Redirect to the flight search results page.
*/
var _redirectToFlightSearch = function (url, lcbUrl) {
// disable search button to prevent duplicate searches
var searchButton = _findSelector('#CHECK_FARES_BUTTON');
searchButton && searchButton.setAttribute('onclick', '');
if (ta.retrieve('isSearchResultsPage')) {
ta.trackEventOnPage(ta.retrieve('searchFormLoadServlet'), 'FL_UpdateSearchForm:NC_SubmitUpdateSearch::CLI', 'FL_UpdateSearchForm', 0, false);
} else {
ta.trackEventOnPage(ta.retrieve('searchFormLoadServlet'), 'FL_SearchForm:NC_SubmitSearch::CLI', 'FL_SearchForm', 0, false);
}
//Only log LCB tracking tree for which a search is kicked off for
_sendLcbTrackingTree();
if (lcbUrl) {
window.open(url);
window.location.href = lcbUrl;
} else {
window.location.assign(url);
}
return false;
};
/**
* Called when the user is not signed in and enters an email address to sign up
* for airwatch using the checkbox.
*/
var _awCheckboxAjax = function (url, lcbUrl) {
var fromCodeField = _findSelector('#rt_' + _getFirstTypeaheadIndex(TRIP_TYPES.ROUND_TRIP) + '_airportFromCode');
var toCodeField = _findSelector('#rt_' + _getFirstTypeaheadIndex(TRIP_TYPES.ROUND_TRIP) + '_airportToCode');
var fromCode = fromCodeField ? fromCodeField.value : '';
var toCode = toCodeField ? toCodeField.value : '';
var email = _findSelector('#airwatchCheckboxEmail') ? _findSelector('#airwatchCheckboxEmail').value : '';
var token = ta.retrieve('flights.securityToken');
var useHTTPS = !ta.retrieve('isLoggedIn');
var ajaxUrl = _getAwAjaxUrl(useHTTPS);
var data = {
to: toCode,
from: fromCode,
pid: AIRWATCH_PID,
token: token
};
if (useHTTPS && email) {
data.email_SIGNIN = email;
}
new Request.JSON({
url: ajaxUrl,
method: 'POST',
data: data,
async: false,
onFailure: function () {
ta.trackEventOnPage(ta.retrieve('searchFormLoadServlet'), 'FL_temp_AirWatch_ServerError', '', AIRWATCH_PID, true);
_redirectToFlightSearch(url, lcbUrl);
},
onError: function () {
ta.trackEventOnPage(ta.retrieve('searchFormLoadServlet'), 'FL_temp_AirWatch_ServerError', '', AIRWATCH_PID, true);
_redirectToFlightSearch(url, lcbUrl);
},
onSuccess: function (responseJSON) {
var status = responseJSON.status;
if (status === 'success') {
url += '&forceAwConf=true';
if (m_fltSearchForm.canUseLocalStore() && responseJSON.tamgsubsHtml) {
localStorage.setItem('airwatch_signup.' + fromCode + toCode, 'true');
localStorage.setItem('aw-checkbox-cross-sub-placement', responseJSON.tamgsubsHtml);
}
_redirectToFlightSearch(url, lcbUrl);
}
else {
ta.trackEventOnPage(ta.retrieve('searchFormLoadServlet'), 'FL_temp_AirWatch_ServerError', '', AIRWATCH_PID, true);
_redirectToFlightSearch(url, lcbUrl);
}
}
}).send();
};
var _sendLcbTrackingTree = function () {
var lcbTrkContainer = _findSelector('.lcbTrackTreeDataContainer');
if (lcbTrkContainer == null) {
return;
}
var lcbTrkTree = _findSelector('.impressionTrackingTree');
if (lcbTrkTree != null) {
ta.trkImpTree(lcbTrkTree);
}
};
/**
* Event handler for when the search button is clicked.
*/
var _doSearch = function (event) {
try {
// stop propagation (ie6 needs this)
new Event(event).stop();
} catch (e) {
// ignore exception
}
if (!m_active) {
return false;
}
if (!_validateInput(false).valid) {
return false;
}
m_fltSearchForm.keepTravelerParams();
var geo = _getGeoCode();
var cos = _findSelector('#cabin').value;
var nonstop = (_findSelector('#nonStopOnly').checked) ? 'yes' : 'no';
var includeNearby = (_findSelector('#includeNearby').checked) ? 'yes' : 'no';
if (m_tripType === TRIP_TYPES.MULTI_CITY) {
cos = _findSelector('#mc_cabin').value;
nonstop = (_findSelector('#mc_nonStopOnly').checked) ? 'yes' : 'no';
}
if (includeNearby === 'yes') {
ta.trackEventOnPage('FL_Search_Form_Large', 'FL_SF_IncludeNearBy', '', 0, false);
}
if (nonstop === 'yes') {
ta.trackEventOnPage('FL_Search_Form_Large', 'FL_SF_PreferNonStopFL', '', 0, false);
}
var params = {
geo: geo,
cos: cos,
nonstop: nonstop
};
//Travelers
params = Object.merge(m_fltSearchForm.getTravelersUrlParams(), params);
//Airports
var airports = _getAirportUrlParams();
var numAirports = Object.keys(airports).length;
params = Object.merge(airports, params);
//Nearby
var nearby = {};
for (var i = 0; i < numAirports; i++) {
nearby['nearby' + i] = includeNearby;
}
params = Object.merge(nearby, params);
//Dates
var dates = _getDatesUrlParams(numAirports / 2);
var numDates = Object.keys(dates).length;
params = Object.merge(dates, params);
//Time
var times = {};
for (i = 0; i < numDates; i++) {
times['time' + i] = '0024';
}
var searchServlet = '/' + (ta.retrieve("flights.searchResultsServlet") || "CheapFlightsSearchResults") + '?';
var url = m_fltSearchForm.buildQueryString(searchServlet, params);
var lcbUrl = _getProvidersWindowUrl();
var doAwCheckboxAjax = false;
var isAirwatchVisible =_findSelector('#airwatch') && _isVisible('airwatch');
// add in parameters for the AirWatch checkbox
if (!ta.retrieve('suppressAWCheapFlights')
&& isAirwatchVisible
&& _findSelector('#airwatchCheckbox')
&& _findSelector('#airwatchCheckbox').checked) {
ta.trackEventOnPage(ta.retrieve('searchFormLoadServlet'), 'FL_SubCheckboxFL:NC_AirWatchCheckBox::CLI', 'FL_SubCheckboxFL', AIRWATCH_PID, false);
if (ta.retrieve('airm.isStreamlinedAirwatch')) {
url += '&aw=true';
if (_findSelector('#airwatchCheckboxEmail') && _findSelector('#airwatchCheckboxEmail').value) {
doAwCheckboxAjax = true;
}
} else {
url += '&aw=true&pid=' + AIRWATCH_PID;
}
}
if (doAwCheckboxAjax) {
_awCheckboxAjax(url, lcbUrl);
} else {
_redirectToFlightSearch(url, lcbUrl);
}
};
/**
* Closes the more option section.
*/
var _closeMoreOptions = function () {
ta.trackEventOnPage('FL_Search_Form_Large', 'FL_SF_LessOptions', '', 0, false);
_findSelector('#mo-link-line').show();
_findSelector('#more-options').hide();
m_moreOptionsOpen = false;
};
/**
* Handler for when the flexible dates link is clicked. Performs a fare calendar search.
*/
var _doFareCalendarSearch = function (event) {
ta.trackEventOnPage('FL_Search_Form_Large', 'FL_SF_DatesareFlexible', '', 0, false);
try {
new Event(event).preventDefault();
} catch (e) {
// ignore exception
}
if (!_validateInput(true).valid) {
return false;
}
var rtTypeaheadIndex = _getFirstTypeaheadIndex(TRIP_TYPES.ROUND_TRIP);
// redirecting, build appropriate url
var url = "/FareCalendar";
url += "-a_airport0." + _findSelector('#rt_' + rtTypeaheadIndex + '_airportFromCode').value +
"-a_airport1." + _findSelector('#rt_' + rtTypeaheadIndex + '_airportToCode').value +
"-a_date0." + m_fltSearchForm.formatDateMetaUrl(m_fltSearchForm.buildDate('rt_leaveMonth', 'rt_leaveDay')) +
"&m=11952&adults=" + m_fltSearchForm.getAdultsAndSeniors();
// IE 8 doesn't send referer header, so check page and specify cal view manually if necessary
if (window.location.href.indexOf("/Flights") > 0
|| window.location.href.indexOf("/CheapFlights") > 0) {
url += "&view=cal";
}
var isOpener = (window.opener && !window.opener.closed && !document.getElementById('neFlights'));
var opener = (isOpener) ? window.opener : window;
opener.focus();
opener.location.assign(url);
if (isOpener) {
opener.close();
}
return false;
};
var _handleSelectChange = function (elem) {
m_fltSearchForm.handleSelectChange(elem);
};
var _hideTravelerPopup = function () {
m_fltSearchForm.hideTravelerPopup();
};
/**
* Sets values on page load/reload.
*/
var _ready = function () {
m_fltSearchForm.init(widget, _doSearch);
//Set page in-date from date-picker fields
var rtCalOffset = _getFirstCalIndex(TRIP_TYPES.ROUND_TRIP);
var inDate = ta.widgets.calendar.dateFromString(_findSelector('#date_picker_in_' + rtCalOffset).innerHTML);
if (inDate) {
ta.util.date.normalize(inDate);
ta.widgets.calendar.setPageInDate(inDate);
}
//Set page out-date from date-picker fields
var outDate = ta.widgets.calendar.dateFromString(_findSelector('#date_picker_out_' + rtCalOffset).innerHTML);
if (outDate) {
ta.util.date.normalize(outDate);
ta.widgets.calendar.setPageOutDate(outDate);
}
// Init trip type tab selection
_setSearchType(m_tripType);
//Setting session defaults
if (m_fltSearchForm.canUseLocalStore() && localStorage.getItem("session-flight-cabin-value")) {
var cabinValue = localStorage.getItem("session-flight-cabin-value");
_findSelector('#cabin').value = cabinValue;
_findSelector('#mc_cabin').value = cabinValue;
}
//Hide more options button if more-options content is showing.
if(m_moreOptionsOpen) {
_findSelector('#mo-link-line').hide();
}
m_fltSearchForm.restoreTravelerParams();
};
ta.queueForReady(function () {
m_active = true;
_ready();
});
return {
typeahead: _typeahead,
pickdate: _pickdate,
doSearch: _doSearch,
doFareCalendarSearch: _doFareCalendarSearch,
setSearchType: _setSearchType,
handleSelectChange: _handleSelectChange,
showMoreOptions: _showMoreOptions,
closeMoreOptions: _closeMoreOptions,
onAirWatchCheckbox: _onAirWatchCheckbox,
cabinSelectChange: _cabinSelectChange,
selectAll: _selectAll,
showTravelerPopup: m_fltSearchForm.showTravelerPopup,
hideTravelerPopup: _hideTravelerPopup,
increaseTravelerNumber: m_fltSearchForm.increaseTravelerNumber,
decreaseTravelerNumber: m_fltSearchForm.decreaseTravelerNumber
};
});
});require(['ta/prwidgets'], function(widgets) {
var define = widgets.define.bind(widgets,'homepage_tripadvisor_footer_links','handlers',['deferred/HelpCenterOverlay','handlers']);
define([
"widget", 'lib/jquery-amd',
"ta/util/Cookie", 'ta/util/ActionRecord'
], function(
widget, $,
taCookie, Track
) {
'use strict';
// called onclick by the wrapper div
function _checkForPid(event) {
var target = event ? (event.srcElement || event.target) : null;
if (target && target.getAttribute("data-pid")) {
taCookie.setPIDCookie(target.getAttribute("data-pid"));
}
}
$(widget.element).on("click", '.js_click[data-id="help_center"]', function(){
var el = this;
var url = $(el).data('url');
widget.require(['trjs!deferred/HelpCenterOverlay', 'trcss!src/build/required/help_center_overlay'], function(o) {
o.open(el, url);
});
});
$(widget.element).on("click", ".footerLink", function(event) {
var actionPrefix = "Footer_";
var link = $(this);
var action = link.data('trackAction');
if (!action) {
return;
}
var opensNewWindow = ("_blank" == link.attr("target"));
if (link.is("a") && !opensNewWindow) {
event.preventDefault();
Track.record(actionPrefix + action)
.always(function() {
window.location = link.attr("href");
});
} else {
Track.record(actionPrefix + action);
}
});
return {
checkForPid : _checkForPid
};
});
});require(['ta/prwidgets'], function(widgets) {
var define = widgets.define.bind(widgets,'homepage_footer_pickers','handlers',['handlers']);
/* jshint newcap:false */
define([
'widget', 'lib/jquery-amd',
'ta/Core/TA.Record'
], function(
widget, $,
taRecord
) {
'use strict';
function _dropdown(evnt, elmt, widgetType, data) {
evnt && evnt.stopPropagation();
elmt.onclick = null;
widget.require([
'trjs!styleguide/overlays/Flyout',
'trjs!overlays/support/widgets', 'trjs!overlays/support/remoteWidget',
'trcss!src/build/styleguide/ui_overlays/flyouts'
], function(Flyout, Widget, RemoteWidget) {
Flyout(evnt, elmt, Widget(widget), RemoteWidget(widgetType, {}, data));
});
}
return {
showPos: function _togglePosPickerOverlay(evnt, elmt){
var posDataEl = $(widget.element).find('.point_of_sale_data');
_dropdown(
evnt, elmt, 'POS_PICKER',
{
pageFeature: posDataEl.data('page-feature') || window.pageFeature || '',
localeCodes: posDataEl.data('locale-codes') || window.localeCodes || '',
relativeUrl: document.location.pathname + (document.location.search||'') + (document.location.hash||'')
}
);
var currLocale = posDataEl.data('currentLocale');
taRecord.trackEventOnPage('POS_PICKER', 'Open', currLocale);
},
showCurrency : function(evnt, elmt){
_dropdown(evnt, elmt, 'CURRENCY_PICKER');
}
};
});
});require(['ta/prwidgets'], function(widgets) {
var define = widgets.define.bind(widgets,'homepage_tripadvisor_horizontal_footer_links','handlers',['handlers']);
define(["widget", 'lib/jquery-amd', "ta/popups/PopUtil", 'ta/util/ActionRecord'],
function(widget, $, popUtil, Track) {
'use strict';
$(widget.element).on('click', '.js_click[data-id="cookie-consent"]', function () {
popUtil.cookieSettingsPanel();
});
$(widget.element).on('click', '.footer_link', function(event) {
var elmt = $(this);
var actionPrefix = "Footer_";
var action = elmt.data('trackAction');
if (!action) {
return;
}
if (elmt.is('a')) {
event.preventDefault();
Track.record(actionPrefix + action)
.always(function() {
window.location = elmt[0].href;
});
}
Track.record(actionPrefix + action);
});
});});
define('common/Radio', ['vanillajs'], function(){
var RADIOS = {};
var SPLITTER = /\s+/;
function Radio(channel){
this.channel = channel;
this.events = {};
this.replies = {};
}
Radio.prototype.reply = function(name, callback, context){
name.split(SPLITTER).forEach(function(n){
this.replies[n] || (this.replies[n] = []);
this.replies[n].push({
callback: callback,
context: context || this
});
}, this);
return this;
};
Radio.prototype.request = function(name){
if (!this.replies[name]){ return null; }
var args = [].slice.call(arguments, 1);
var r = this.replies[name];
r = r[r.length - 1];
return r.callback.apply(r.context, args);
};
Radio.prototype.requestMany = function(name){
if (!this.replies[name]){ return null; }
var args = [].slice.call(arguments, 1);
return this.replies[name].map(function(r){
return r.callback.apply(r.context, args);
});
};
Radio.prototype.requestAll = function(name, expected){
if (!this.replies[name]){ return true; }
var args = [].slice.call(arguments, 1);
var fn = typeof expected == 'function' ? expected : function(v){ return v === expected; };
return this.replies[name].reduce(function(b, r){
return b && fn(r.callback.apply(r.context, args));
}, true);
};
Radio.prototype.requestEvery = Radio.prototype.requestAll;
Radio.prototype.requestAny = function(name, expected){
if (!this.replies[name]){ return false; }
var args = [].slice.call(arguments, 1);
var fn = typeof expected == 'function' ? expected : function(v){ return v === expected; };
return this.replies[name].reduce(function(b, r){
return b || fn(r.callback.apply(r.context, args));
}, false);
};
Radio.prototype.requestSome = Radio.prototype.requestAny;
Radio.prototype.on = function(name, callback, context){
name.split(SPLITTER).forEach(function(n){
this.events[n] || (this.events[n] = []);
this.events[n].push({
callback: callback,
context: context || this
});
}, this);
return this;
};
Radio.prototype.once = function(name, callback, context){
name.split(SPLITTER).forEach(function(n){
this.events[n] || (this.events[n] = []);
this.events[n].push({
callback: callback,
context: context || this,
single: true
});
}, this);
return this;
};
Radio.prototype.one = Radio.prototype.once;
Radio.prototype.off = function(name, callback){
name.split(SPLITTER).forEach(function(n){
if (!this.events[n]) {return;}
this.events[n] = this.events[n].filter(function(o){
return callback && o.callback != callback;
});
}, this);
return this;
};
Radio.prototype.trigger = function(name){
if (!this.events[name]) {return;}
var args = [].slice.call(arguments, 1);
this.events[name] = this.events[name].filter(function(o){
o.callback.apply(o.callback, args);
return !o.single;
});
return this;
};
Radio.prototype.emit = Radio.prototype.trigger;
return function(channel){
return (RADIOS[channel] || (RADIOS[channel] = new Radio(channel)));
};
});
require(['ta/p13n/placements'], function(placements) {
var define = placements.define.bind(placements,'global_nav','handlers',['deferred/lateHandlers','handlers']);
/* jshint newcap:false */
define([
'placement',
'lib/jquery-amd',
'common/Radio'
], function(
placement,
$,
Radio
) {
'use strict';
var placementEl = $('#' + placement.id);
var radio = Radio('global-nav');
var oldOverlay = null;
var mastheadSavesApp = null;
// Trigger in 'placeemnts/masthead_search/handlers'
radio.reply('is-hidden-link', function() {
var allEls = placementEl.find(".global-nav-links-container li");
var collapsedEls = allEls.filter(function(idx, el) { return $(el).offset().top > allEls.offset().top; });
return collapsedEls.length > 0;
});
// Trigger in 'placeemnts/global_nav_action_trips/handlers'
radio.on('run-my-trips-test-3', function() {
openMyTrips(false, true);
});
// Update Global Nav content
var _onSuccessLoginRefresh = (function(response) {
window.userLoggedIn = true;
// Update the masthead, but keep some of the old content.
// This is because the AJAX servlet does not generate the correct content
// for the original page (instead, it generates it for /MetaPlacementAjax).
// Note this uses the IDs to match each "old" element with its corresponding "new" element.
var oldMenus = $("<div>").append(placementEl.find('.global-nav-no-refresh'));
placementEl.html(response);
placementEl.find('.global-nav-no-refresh').each(function(idx, el) {
$(el).replaceWith(oldMenus.find('#' + el.getAttribute('id'))[0]);
});
if (oldOverlay) {
oldOverlay.hide('replace-el');
}
}).bind(placementEl);
var _getRequestOptionsForLoginRefresh = function() {
return {
// This should not be necessary, but for some reason placements
// seems tightly coupled with the location store
skipLocation: placement.location_id <= 0,
returnTo: document.location.pathname + (document.location.search||'') + (document.location.hash||'')
};
};
// When login state changes, request the updated global nav
$(document).on('regOverlay:success', function() {
placement.requestAJAXPlacement( _onSuccessLoginRefresh, null, _getRequestOptionsForLoginRefresh());
});
// When mousing over the global nav links, show their submenus
placementEl.on('mouseenter', '.global-nav-links-container [data-element]', function(evt) {
var elmt = $(this);
var linkEl = elmt.find('a').first();
if (oldOverlay) {
oldOverlay.hide('new-overlay');
}
// Is there a submenu to show?
var menuEl = placementEl.find(elmt.data('element')).clone();
if (!menuEl.length) {
return;
}
var reqs = ['trjs!overlays/Overlay',
'trjs!overlays/styles/flyout',
'trjs!overlays/options/closeOnMouseAway',
'trjs!overlays/position',
'trjs!overlays/options/destroyOnHide'
];
require(reqs, function(Overlay, Flyout, CloseOnMouseAway, Position, DestroyOnHide) {
var overlay = new Overlay(elmt[0], new Flyout(menuEl[0], 'global-nav-flyout global-nav-menu'), CloseOnMouseAway, Position.bottomRight([0, 1]), DestroyOnHide);
overlay.domParent = placementEl.find('.global-nav-overlays-container')[0];
overlay.show();
// For tracking clicks to submenu links, we add an attribute to the link being hovered over so we can retrieve
// the tracking prefix from the link's tracking-label attribute.
linkEl.attr('data-active-menu-trigger', true);
$(evt.currentTarget).find('.ui_tab').addClass('hovering');
overlay.on('hide', function() {
// Remove attribute used for tracking
linkEl.removeAttr('data-active-menu-trigger');
$(evt.currentTarget).find('.ui_tab').removeClass('hovering');
});
oldOverlay = overlay;
});
});
// When mousing over a menu in the dropdown, open the submenu
placementEl.on('mouseenter', '.expandSubItem', function(evt) {
var elmt = $(this);
// CX-2715 avoid opening a duplicate submenu
if (elmt.hasClass('active')) {
return;
}
elmt.addClass('active');
elmt.siblings('.expandSubItem').removeClass('active');
var submenuEl = elmt.find('.secondSubNav').clone();
var targetEl = elmt.parents('.ui_overlay');
var reqs = ['trjs!overlays/Overlay',
'trjs!overlays/styles/flyout',
'trjs!overlays/options/closeOnMouseOut',
'trjs!overlays/options/destroyOnHide'
];
require(reqs, function(Overlay, Flyout, CloseOnMouseOut, DestroyOnHide) {
var overlay = new Overlay(elmt[0], new Flyout(submenuEl[0]), CloseOnMouseOut, DestroyOnHide);
overlay.domParent = targetEl[0];
submenuEl.css('display', 'block');
overlay.show();
});
});
// If we're opening the more menu, add any elements that are hidden or collapsed due to space
placementEl.on('mouseenter', '.global-nav-links-ellipsis', function(evt) {
var elmt = $(this);
if (oldOverlay) {
oldOverlay.hide('new-overlay');
}
var allEls = placementEl.find(".global-nav-links-container li");
var hiddenEls = allEls.filter(":hidden");
var collapsedEls = allEls.filter(function(idx, el) { return $(el).offset().top > allEls.offset().top; });
var elsToShow = $().add(hiddenEls).add(collapsedEls).clone();
var menuEl = placementEl.find('.global-nav-links-menu-more').clone();
menuEl.prepend(elsToShow);
var reqs = ['trjs!overlays/Overlay',
'trjs!overlays/styles/flyout',
'trjs!overlays/options/closeOnMouseAway',
'trjs!overlays/position',
'trjs!overlays/options/destroyOnHide'
];
require(reqs, function(Overlay, Flyout, CloseOnMouseAway, Position, DestroyOnHide) {
var overlay = new Overlay(elmt[0], new Flyout(menuEl[0], 'global-nav-flyout global-nav-menu'), CloseOnMouseAway, Position.bottomRight([0, 1]), DestroyOnHide);
overlay.domParent = placementEl.find('.global-nav-overlays-container')[0];
overlay.show();
elmt.find('.ui_tab').addClass('hovering');
overlay.on('hide', function() {
elmt.find('.ui_tab').removeClass('hovering');
});
oldOverlay = overlay;
});
});
// When clicking on my trips
